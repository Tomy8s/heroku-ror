<table>
  <thead>
    <td>First name</td><td>Surname</td><td>Username</td><td>D.o.B</td><td>Password</td><td>gender</td><td>Email</td>
  </thead>
  <tbody>
    <% @user.each do |u| %>
      <tr>
        <td>
          <%= u.fname %>
        </td>
        <td>
          <%= u.sname %>
        </td>
        <td>
          <%= u.uname %>
        </td>
        <td>
          <%= u.dob %>
        </td>
        <td>
          <%= u.password_digest %>
        </td>
        <td>
          <%= u.gender %>
        </td>
        <td>
          <%= u.email %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>